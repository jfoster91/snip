{
  "manifest_version": 2,
  "name": "Basic",
  "version": "0.1",
  "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": ["content.js"]
      }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  
  "browser_action": {
    "default_title": "basic",
    "default_popup": "popup.html"
  }
}
